generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Diagnosis {
  id              Int      @id @default(autoincrement())
  name            String
  symptoms        Symptom[]
  severity        Severity    @default(MEDIUM)
}

model Symptom {
  symptomId       Int      @id @default(autoincrement())
  name            String
  diagnosis       Diagnosis[]
}

enum Severity {
  HIGH
  MEDIUM
  LOW
}
